{
  "version": 2,
  "builds": [
    {
      "src": "server/api/**/*.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/notify",
      "dest": "/server/api/notify.ts",
      "methods": ["POST", "OPTIONS"]
    },
    {
      "src": "/api/?",
      "dest": "/server/api/index.ts",
      "methods": ["GET", "OPTIONS"]
    }
  ]
}
