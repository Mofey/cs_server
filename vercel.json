{
  "version": 2,
  "builds": [
    {
      "src": "server/api/**/*.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server/api/$1.ts",
      "methods": ["GET", "POST", "OPTIONS"]
    },
    {
      "src": "/api",
      "dest": "/server/api/index.ts",
      "methods": ["GET", "POST", "OPTIONS"]
    }
  ]
}
